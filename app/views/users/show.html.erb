
  <div class="row">
    <div class="col-lg-12">
      <h3 class="page-header"><i class="fa fa-user-md"></i> Profile</h3>
      <ol class="breadcrumb">
        <li><i class="fa fa-home"></i><a href="/">Home</a></li>
        <li><i class="fa fa-user-md"></i>Profile</li>
      </ol>
    </div>
  </div>
  <% @users.each do |user| %>
  <div class="row">
    <!-- profile-widget -->
    <div class="col-lg-12">
      <div class="profile-widget profile-widget-info">
        <div class="panel-body">
          <div class="col-lg-2 col-sm-2">
            <h4><%= user.name %></h4>               
            <div class="follow-ava">
              <img src="<%= user.avatar %>" alt="">
            </div>
            <h6><%= user.email %></h6>
          </div>
          <div class="col-lg-4 col-sm-4 follow-info">
            <p>Hello <%= user.name %></p>
            <p><%= user.name %></p>
            <p><i class="fa fa-twitter"><%= user.email %></i></p>
            <h6>
              <span><i class="icon_clock_alt"></i>Signed in Counts: <%= user.sign_in_count %></span>
              <span><i class="icon_calendar"></i>Last signed in: <%= user.last_sign_in_at %></span>
            </h6>
          </div>
          
          <div class="col-lg-2 col-sm-6 follow-info weather-category">
            <ul>
              <li class="active">

                <i class="fa fa-bell fa-2x"> </i><br>

                <a href="/users/edit" class="btn btn-primary">Edit Data</a>
              </li>

            </ul>
          </div>
          <div class="col-lg-4 col-sm-6 ">
            <ul>
              <li class="active">
                <p id="notice"><%= notice %></p>
              </li>

            </ul>
          </div>
          
        </div>
      </div>
    </div>
  </div>
<% end %>
