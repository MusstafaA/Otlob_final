
		<div class="col-lg-12" style="margin-top:20px;">
			<section class="panel">
				<header class="panel-heading tab-bg-info">
					<ul class="nav nav-tabs">
						<li class="active">
							<a data-toggle="tab" href="#latest-orders">
								<i class="icon-home"></i>
								Latest orders
							</a>
						</li>
						<li>
							<a data-toggle="tab" href="#friends-activity">
								<i class="icon-user"></i>
								Friends Activity
							</a>
						</li>
					</ul>
				</header>
				<div class="panel-body">
					<div class="tab-content">
						<!-- latest-orders -->
						<div id="latest-orders" class="tab-pane active">
							<div class="profile-activity">                                          
								<div class="act-time">                                      
									<div class="activity-body act-in">
										<span class="arrow"></span>
										<div class="text">
											<%  @orders.each do |order| %>
     
											
											<p class="attribution"><%= link_to  order.for+"  on  "+order.created_at.to_s  , order %>
											</p>
											
											 <% end %>
										</div>
									</div>
								</div>

							</div>
						</div>

						<!-- friends-activity -->
						<div id="friends-activity" class="tab-pane">
							<section class="panel">
								<div class="profile-activity">                                          
								<div class="act-time">                                      
									<div class="activity-body act-in">
										<span class="arrow"></span>
	

										<div class="text" data-behavior='notifeeds'>										
											<p data-behavior='notification-feeds'></p>
										</div>
									</div>
								</div>
							</div>
							</section>
							<section>
								<div class="row">                                              
								</div>
							</section>
						</div>
						
					</div>
				</div>
			</section>
		</div>
